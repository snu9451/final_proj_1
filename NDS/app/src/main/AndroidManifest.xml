<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nds">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NDS">
        <activity android:name=".InsertCoinActivity"></activity>
        <activity android:name=".MyLikeActivity" />
        <activity android:name=".MyWalletActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MyTradeActivity" />
        <activity android:name=".MyErrandActivity" />
        <activity android:name=".UpdateNicknameActivity" />
        <activity android:name=".MyInfoActivity" />
        <activity android:name=".MainActivity"></activity>
    </application>

</manifest>